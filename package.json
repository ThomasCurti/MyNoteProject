{
  "name": "mynoteproject",
  "version": "1.0.0",
  "description": "Test Template for fastify",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tap --no-check-coverage --node-arg=\"-r\" --node-arg=\"ts-node/register\" \"src/test/**/*.test.ts\"",
    "build": "tsc",
    "start": "rm -rf _dist && tsc && ts-node _dist/app.js",
    "dev": "rm -rf _dist && tsc && ts-node-dev --respawn _dist/app.js"
  },
